def bbsort(lst):
    '''
    this is bubble sort algorithm, works great for small datasets
    '''
    
    for i in range(0,len(lst)):
        for j in range(0,len(lst)-1):
            if lst[j] > lst[j+1]:
                lst[j], lst[j+1] = lst[j+1],lst[j]            
    return lst
    
    
